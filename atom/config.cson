"*":
  core:
    disabledPackages: [
      "whitespace"
      "autocomplete-plus"
    ]
    telemetryConsent: "no"
  editor:
    fontSize: 18
    preferredLineLength: 79
    softWrap: true
    softWrapAtPreferredLineLength: true
    tabLength: 2
    tabType: "soft"
  "spell-check":
    addKnownWords: true
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "source.rst"
      "text.restructuredtext"
      "source.c"
      "source.python"
    ]
    knownWords: [
      "decrypt"
      "adjacencies"
      "initilizer"
      "cyber"
    ]
  welcome:
    showOnStartup: false
".python.regexp.source":
  editor:
    tabLength: 4
".python.source":
  editor:
    tabLength: 4
